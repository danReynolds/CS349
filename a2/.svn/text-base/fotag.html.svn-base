<!-- page should match the screen's width in device independent pixels, reflowing the content -->
<meta name="viewport" content="width=device-width, initial-scale=1">
<head lang="en">
    <meta charset="UTF-8">
    <title>Fotag</title>

    <link rel="stylesheet" href="css/fotag.css"/>
    <style>
        .marking-button {
            float:right;
        }
        .marking-div-clear {
            clear:both;
        }
    </style>
</head>
<body>

<div id="app-container">
</div>

<template id="header-template">
    <div class="header-content">
        <h1>Fotag</h1>
    </div>
</template>

